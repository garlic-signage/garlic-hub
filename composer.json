{
  "require": {
        "slim/slim": "4.*",
        "slim/psr7": "^1.7",
        "php-di/php-di": "^7.0",
        "mustache/mustache": "^2.14",
        "vlucas/phpdotenv": "^5.6",
        "monolog/monolog": "^3.7",
        "slim/flash": "^0.4.0",
        "league/flysystem": "^3.29",
      "ext-intl": "*",
        "phpfastcache/phpfastcache": "^9.2",
        "doctrine/dbal": "^4.2",
      "symfony/console": "^7.2",
      "league/oauth2-server": "^9.1",
      "ext-openssl": "*",
      "defuse/php-encryption": "^2.4",
      "slim/http": "^1.4",
      "ramsey/uuid": "^4.7",
      "ext-fileinfo": "*",
    "ext-imagick": "*",
    "league/flysystem-ziparchive": "^3.29"
  },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5",
        "phpstan/phpstan": "^2.0",
        "phpmd/phpmd": "^2.15",
        "phpstan/phpstan-phpunit": "^2.0"
    }
}
