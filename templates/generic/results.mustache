	<ul class="results-listing">
		<li>
			<ul class="results-header">
				{{#elements_result_header}}
				<li class="{{{CONTROL_NAME}}}">
					{{#if_sortable}}
					<a href="{{{LINK_CONTROL_SORT_ORDER}}}" class="header_sort_column" id="{{{SORT_CONTROL_NAME}}}_sort_column">{{{LANG_CONTROL_NAME}}}</a> {{{SORTABLE_ORDER}}}
					{{/if_sortable}}
					{{{LANG_CONTROL_NAME_2}}}
				</li>
				{{/elements_result_header}}
				<li class="actions"></li>
			</ul>
		</li>
		{{#elements_results}}
		<li>
			<ul style="{{{MARKED_AS_DISABLED}}}" class="results-body" data-id="{{{UNIT_ID}}}">
				{{#elements_result_element}}
				<li class="{{{CONTROL_NAME_BODY}}}">
					{{#is_link}}
					<a href="{{{CONTROL_ELEMENT_VALUE_LINK}}}" id="{{{CONTROL_ELEMENT_VALUE_ID}}}" class="{{{CONTROL_ELEMENT_VALUE_CLASS}}}" title="{{{CONTROL_ELEMENT_VALUE_TITLE}}}">{{{CONTROL_ELEMENT_VALUE_NAME}}}</a>
					{{/is_link}}

					{{#is_text}}
					{{{CONTROL_ELEMENT_VALUE}}}
					{{/is_text}}

					{{#is_icon_column}}
                    <div class="{{{CONTROL_ICON_CLASSNAME}}}"><span title="{{{CONTROL_ELEMENT_MESSAGE}}}"></span></div>
					{{/is_icon_column}}

					{{#is_bullet}}
                    <div class="thymian-icon thymian-icon-{{{BULLET_COLOR}}}" title="{{{LANG_MONITORING}}}">&nbsp;</div>
					{{/is_bullet}}

					{{#is_checkbox}}
						<label><input type="checkbox" {{{SELECT_DISABLED}}} title="{{{LANG_CHECKBOX_NAME}}}"></label>
					{{/is_checkbox}}

					{{#is_icon_column_linked}}
					<div class="{{{CONTROL_ICON_CLASSNAME_LINKED}}}"><a href="{{{CONTROL_ELEMENT_LINK}}}" title="{{{CONTROL_ELEMENT_MESSAGE_LINKED}}}" class=""></a></div>
					{{/is_icon_column_linked}}

					{{#is_expand_collapse}}
					<details>
						<summary>{{{CONTROL_ELEMENT_MESSAGE_COLLAPSED}}}</summary>
						<div>
							{{{CONTROL_ELEMENT_MESSAGE_EXPANDED}}}
						</div>
					</details>
					{{/is_expand_collapse}}
					{{#is_UID}}
						<a data-uid="{{{OWNER_UID}}}')" title="{{{OWNER_NAME}}}">{{{OWNER_NAME}}}</a>
					{{/is_UID}}
					{{#is_UID_ShowOnly}}
					{{{OWNER_NAME}}}
					{{/is_UID_ShowOnly}}
				</li>
				{{/elements_result_element}}
				<li class="actions">
                    <ul>
                        {{#has_action}}
                        <li>
                            <a href="{{{LINK_ACTION}}}" title="{{{LANG_ACTION}}}"
							   data-action="{{{ACTION_NAME}}}" data-action-id="{{{SUBMENU_ID}}}"
							   style="{{{ACTION_INLINE_CSS}}}"
							   class="bi bi-{{{ACTION_ICON_CLASS}}} {{{ACTION_CLASS}}}"></a>
                        </li>
						{{/has_action}}
						{{#has_delete}}
                        <li>
                            <a href="{{{LINK_DELETE_ACTION}}}" data-delete-id="{{{DELETE_ID}}}" onclick="return confirm_delete('{{{LANG_CONFIRM_DELETE}}}')" title="{{{LANG_DELETE}}}" class="bi bi-trash {{{ELEMENT_DELETE_CLASS}}}">&nbsp;&nbsp;</a>
                        </li>
						{{/has_delete}}
                    </ul>
				</li>
			</ul>
		</li>
	{{/elements_results}}
	</ul>
